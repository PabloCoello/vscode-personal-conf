{
  "$schema": "vscode://schemas/color-theme",
  "name": "BeTheme Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#1A191E",
    "editorCursor.foreground": "#4F80DD",
    "editor.lineHighlightBackground": "#F5F6F7A6",
    "editor.selectionBackground": "#4F80DD33",
    "editor.inactiveSelectionBackground": "#4F80DD22",
    "editor.wordHighlightBackground": "#E5B15D22",
    "editor.wordHighlightStrongBackground": "#E2705633",
    "sideBar.background": "#F8F9FA",
    "sideBar.foreground": "#1A191E",
    "activityBar.background": "#F8F9FA",
    "activityBar.foreground": "#1A191E",
    "activityBarBadge.background": "#4F80DD",
    "activityBarBadge.foreground": "#FFFFFF",
    "statusBar.background": "#F8F9FA",
    "statusBar.foreground": "#1A191E",
    "titleBar.activeBackground": "#F8F9FA",
    "titleBar.activeForeground": "#1A191E",
    "titleBar.inactiveForeground": "#6B7280",
    "panel.background": "#F8F9FA",
    "panel.border": "#E5E7EB",
    "input.background": "#FFFFFF",
    "input.foreground": "#1A191E",
    "input.placeholderForeground": "#9CA3AF",
    "input.border": "#D1D5DB",
    "list.hoverBackground": "#F3F4F6",
    "list.activeSelectionBackground": "#E5E7EB",
    "list.activeSelectionForeground": "#1A191E",
    "list.highlightForeground": "#4F80DD",
    "badge.background": "#4F80DD",
    "badge.foreground": "#FFFFFF",
    "tab.activeBackground": "#FFFFFF",
    "tab.inactiveBackground": "#F3F4F6",
    "tab.activeForeground": "#1A191E",
    "tab.inactiveForeground": "#6B7280",
    "tab.border": "#E5E7EB",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E5E7EB",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E5E7EB",
    "diffEditor.insertedTextBackground": "#58B69D22",
    "diffEditor.removedTextBackground": "#E2705622",
    "gitDecoration.addedResourceForeground": "#58B69D",
    "gitDecoration.modifiedResourceForeground": "#E5B15D",
    "gitDecoration.deletedResourceForeground": "#E27056",
    "gitDecoration.untrackedResourceForeground": "#4F80DD",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#1A191E",
    "terminal.ansiBlack": "#1A191E",
    "terminal.ansiRed": "#E27056",
    "terminal.ansiGreen": "#58B69D",
    "terminal.ansiYellow": "#E5B15D",
    "terminal.ansiBlue": "#4F80DD",
    "terminal.ansiMagenta": "#8B6BA8",
    "terminal.ansiCyan": "#4C9AB8",
    "terminal.ansiWhite": "#D1D3D4",
    "terminal.ansiBrightBlack": "#6B7280",
    "terminal.ansiBrightRed": "#F87171",
    "terminal.ansiBrightGreen": "#6EE7B7",
    "terminal.ansiBrightYellow": "#FCD34D",
    "terminal.ansiBrightBlue": "#60A5FA",
    "terminal.ansiBrightMagenta": "#A78BFA",
    "terminal.ansiBrightCyan": "#67E8F9",
    "terminal.ansiBrightWhite": "#F9FAFB",
    "editor.findMatchBackground": "#E5B15D44",
    "editor.findMatchHighlightBackground": "#E5B15D22",
    "editorLineNumber.foreground": "#9CA3AF",
    "editorLineNumber.activeForeground": "#4F80DD",
    "editorIndentGuide.background1": "#E5E7EB",
    "editorIndentGuide.activeBackground1": "#D1D5DB",
    "editorWhitespace.foreground": "#E5E7EB",
    "editorRuler.foreground": "#E5E7EB",
    "scrollbarSlider.background": "#D1D5DB66",
    "scrollbarSlider.hoverBackground": "#D1D5DB99",
    "scrollbarSlider.activeBackground": "#D1D5DBCC"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6B7280",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "meta.object-literal.key",
        "support.variable.property",
        "punctuation"
      ],
      "settings": {
        "foreground": "#1A191E"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#4F80DD"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#4F80DD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python: Classes",
      "scope": [
        "entity.name.type.class.python",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#8B6BA8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python: Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#E5B15D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python: Built-in functions",
      "scope": [
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "Python: Magic methods",
      "scope": [
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Python: Keywords",
      "scope": [
        "keyword.control.import.python",
        "keyword.control.flow.python"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "Python: Self parameter",
      "scope": [
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#8B6BA8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python: Exception types",
      "scope": [
        "support.type.exception.python"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "Python: Constants",
      "scope": [
        "constant.language.python"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "Python: Function parameters",
      "scope": [
        "variable.parameter.function.language.python"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "Python: String formatting",
      "scope": [
        "constant.character.format.placeholder.other.python",
        "meta.format.brace.python"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Python: Module/Package names",
      "scope": [
        "entity.name.namespace.python"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "JSON: Property names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "CSS: Property names",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "CSS: Property values",
      "scope": [
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "HTML: Tag names",
      "scope": [
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "HTML: Attributes",
      "scope": [
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "JavaScript: Object properties",
      "scope": [
        "variable.other.property.js",
        "variable.other.object.property.js"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "JavaScript: Template literals",
      "scope": [
        "string.template.js"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "Python: Docstrings",
      "scope": [
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#58B69D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python: Type hints",
      "scope": [
        "meta.function.parameters.python support.type.python"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "Variable assignments",
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "name": "R: Assignment operators",
      "scope": [
        "keyword.operator.assignment.r"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "R: Function names",
      "scope": [
        "entity.name.function.r",
        "support.function.r"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "R: Built-in functions",
      "scope": [
        "support.function.builtin.r"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "R: Variables",
      "scope": [
        "variable.other.r",
        "variable.language.r"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "R: Parameters and arguments",
      "scope": [
        "variable.parameter.r"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "R: Logical constants",
      "scope": [
        "constant.language.logical.r"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "R: Package names and namespaces",
      "scope": [
        "entity.name.namespace.r",
        "support.class.r"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "R: Pipe operator",
      "scope": [
        "keyword.operator.pipe.r"
      ],
      "settings": {
        "foreground": "#E5B15D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "R: Control flow",
      "scope": [
        "keyword.control.r"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "R: Data types",
      "scope": [
        "storage.type.r"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "R: Numeric constants",
      "scope": [
        "constant.numeric.r"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "R: Strings",
      "scope": [
        "string.quoted.double.r",
        "string.quoted.single.r"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "Markdown: Headings",
      "scope": [
        "markup.heading.markdown",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#4F80DD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Bold text",
      "scope": [
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#E5B15D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown: Italic text",
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#58B69D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Links",
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#4C9AB8"
      }
    },
    {
      "name": "Markdown: Link text",
      "scope": [
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Markdown: Code spans",
      "scope": [
        "markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Markdown: Code blocks",
      "scope": [
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "Markdown: Blockquotes",
      "scope": [
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#E5B15D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Lists",
      "scope": [
        "markup.list.markdown"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Quarto: YAML frontmatter",
      "scope": [
        "meta.embedded.block.frontmatter"
      ],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "name": "Quarto: Code chunk headers",
      "scope": [
        "meta.code-fence.definition.begin.markdown",
        "meta.code-fence.definition.end.markdown"
      ],
      "settings": {
        "foreground": "#8B6BA8"
      }
    },
    {
      "name": "Quarto: Chunk options",
      "scope": [
        "variable.language.fenced.markdown"
      ],
      "settings": {
        "foreground": "#E5B15D"
      }
    },
    {
      "name": "Quarto: Callout blocks",
      "scope": [
        "markup.other.callout.markdown"
      ],
      "settings": {
        "foreground": "#4C9AB8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "YAML: Keys",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#4F80DD"
      }
    },
    {
      "name": "YAML: Values",
      "scope": [
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#58B69D"
      }
    },
    {
      "name": "Punctuation and brackets",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.separator.comma",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "name": "Special characters and symbols",
      "scope": [
        "constant.character.escape",
        "punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#E27056"
      }
    },
    {
      "name": "Error highlighting improvement",
      "scope": [
        "invalid.deprecated",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E27056",
        "fontStyle": "bold"
      }
    }
  ]
}